{
  "name": "ai-dev-system",
  "version": "1.0.0",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "build": "node scripts/build.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "auto-setup": "node scripts/auto-dev-helper.js setup",
    "auto-dev": "node scripts/auto-dev-helper.js",
    "auto-format": "prettier --write . && eslint --fix .",
    "auto-test": "jest --watch --silent",
    "macos-setup": "./scripts/macos-auto-setup.sh",
    "auto-start": "echo y | npm run start",
    "auto-install": "yes | npm install",
    "auto-build": "echo y | npm run build",
    "auto-update": "npm config set yes true && npm update",
    "force-start": "CI=true npm run start",
    "silent-dev": "npm_config_yes=true npm run dev",
    "no-prompt": "DEBIAN_FRONTEND=noninteractive npm run start",
    "full-auto-setup": "./scripts/full-terminal-automation.sh setup",
    "full-auto-install": "./scripts/full-terminal-automation.sh install",
    "full-auto-start": "./scripts/full-terminal-automation.sh start",
    "full-auto-test": "./scripts/full-terminal-automation.sh test",
    "full-auto-git": "./scripts/full-terminal-automation.sh git",
    "full-auto-all": "./scripts/full-terminal-automation.sh all",
    "zero-prompt": "CI=true npm_config_yes=true DEBIAN_FRONTEND=noninteractive npm run start",
    "background-start": "nohup npm start > /dev/null 2>&1 &",
    "daemon-start": "npm start &",
    "quiet-start": "npm start --silent 2>/dev/null",
    "test-automation": "./scripts/test-automation.sh",
    "verify-automation": "./scripts/test-automation.sh && ./scripts/full-terminal-automation.sh setup",
    "deploy": "echo 'Ready for web deployment! Run: npm run deploy:vercel or npm run deploy:railway'",
    "deploy:vercel": "vercel --prod",
    "deploy:railway": "railway deploy",
    "deploy:heroku": "git push heroku main",
    "deploy:docker": "docker build -t ai-dev-system . && docker run -p 80:3000 ai-dev-system",
    "deploy:prepare": "./scripts/deploy-web.sh"
  },
  "keywords": [
    "ai",
    "automation",
    "development",
    "system"
  ],
  "author": "Daniel Kwon",
  "license": "MIT",
  "description": "AI-powered automated development system",
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^2.0.1",
    "openai": "^4.0.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
